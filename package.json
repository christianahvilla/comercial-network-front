{
  "name": "comercial-network-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "1.3.6",
    "@devexpress/dx-react-chart": "^2.4.1",
    "@devexpress/dx-react-chart-material-ui": "^2.4.1",
    "@devexpress/dx-react-core": "^2.4.1",
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.41",
    "axios": "^0.19.2",
    "clsx": "^1.1.0",
    "date-fns": "2.0.0-beta.2",
    "devextreme": "19.2.6",
    "devextreme-react": "^19.2.6",
    "material-table": "^1.57.2",
    "material-ui-icons": "^1.0.0-beta.36",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hook-form": "^4.7.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.4.0"
  },
  "scripts": {
    "start": "CHOKIDAR_USEPOLLING=true react-scripts start",
    "dev:up": "docker-compose -f docker-compose.yml -f dev.yml up",
    "dev:build": "docker-compose -f docker-compose.yml -f dev.yml build",
    "build": "react-scripts build",
    "prod:build": "docker-compose -f docker-compose.yml -f prod.yml  build",
    "prod:up": "docker-compose -f docker-compose.yml -f prod.yml up -d --build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,jsx}' --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,css,scss}'"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier-eslint --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 4
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "prettier-eslint-cli": "^5.0.0"
  }
}
